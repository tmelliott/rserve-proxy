{
  "name": "@rserve-proxy/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx src/db/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "docker:cleanup": "tsx src/scripts/cleanup.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/multipart": "^9.4.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/session": "^11.0.0",
    "@fastify/static": "^8.0.0",
    "@rserve-proxy/shared": "workspace:*",
    "@sinclair/typebox": "^0.34.48",
    "argon2": "^0.41.0",
    "dockerode": "^4.0.0",
    "drizzle-orm": "^0.39.0",
    "fastify": "^5.2.0",
    "nanoid": "^5.0.0",
    "postgres": "^3.4.0",
    "tar-fs": "^3.1.1"
  },
  "devDependencies": {
    "@rserve-proxy/shared": "workspace:*",
    "@types/dockerode": "^3.3.0",
    "@types/tar-fs": "^2.0.4",
    "drizzle-kit": "^0.30.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
